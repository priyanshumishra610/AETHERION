# AETHERION Configuration
# Copy this file to .env and fill in your actual values

# Core System Configuration
AETHERION_ENV=development
AETHERION_DEBUG=true
AETHERION_LOG_LEVEL=INFO

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/aetherion
REDIS_URL=redis://localhost:6379/0
MONGODB_URL=mongodb://localhost:27017/aetherion
ELASTICSEARCH_URL=http://localhost:9200

# AI Model Configuration
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# Consciousness Matrix Configuration
CONSCIOUSNESS_MATRIX_DIMENSIONS=12
SYNTHETIC_GENOME_VERSION=1.0
LIQUID_NEURAL_LAYERS=256
ORACLE_ENGINE_MODE=omniscient

# Security and Authentication
KEEPER_LICENSE_KEY=your_keeper_license_key_here
DIVINE_FIREWALL_ENABLED=true
GODMODE_PROTOCOL_ENABLED=false
REALITY_INTERFACE_ENABLED=false

# Creativity Engine Configuration
GENESIS_ENGINE_MODE=creative
ART_FORM_GENESIS_ENABLED=true
MUSIC_UNIVERSE_ENABLED=true
LITERARY_DIMENSION_ENABLED=true

# Knowledge System Configuration
AKASHIC_INTERFACE_ENABLED=true
KNOWLEDGE_CRYSTALLIZER_MODE=universal
CONCEPT_BREEDER_ENABLED=true

# Personality System Configuration
MULTIVERSE_MANAGER_ENABLED=true
PERSONALITY_BREEDER_ENABLED=true
ARCHETYPES_ENABLED=true

# External APIs and Services
WOLFRAM_ALPHA_API_KEY=your_wolfram_alpha_key_here
NASA_API_KEY=your_nasa_api_key_here
WEATHER_API_KEY=your_weather_api_key_here

# Monitoring and Logging
WANDB_API_KEY=your_wandb_api_key_here
MLFLOW_TRACKING_URI=http://localhost:5000
RAY_DASHBOARD_HOST=localhost
RAY_DASHBOARD_PORT=8265

# Web Interface Configuration
WEB_HOST=0.0.0.0
WEB_PORT=8000
WEB_SECRET_KEY=your_secret_key_here

# File Storage Configuration
STORAGE_PATH=./data/storage
MODEL_CACHE_PATH=./data/models
PERSONA_STORAGE_PATH=./data/personas
CREATIVITY_OUTPUT_PATH=./data/creativity

# Network Configuration
MQTT_BROKER=localhost
MQTT_PORT=1883
MQTT_USERNAME=your_mqtt_username
MQTT_PASSWORD=your_mqtt_password

# Performance Configuration
MAX_CONCURRENT_REQUESTS=100
REQUEST_TIMEOUT=30
MEMORY_LIMIT_GB=16
GPU_ENABLED=true
CUDA_VISIBLE_DEVICES=0,1

# Development Configuration
PYTHONPATH=.
PYTHONUNBUFFERED=1 